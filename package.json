{
  "name": "@programmerraj/sokk",
  "version": "1.0.0",
  "description": "Round/ceil/float to any integer multiple",
  "keywords": [
    "typescript",
    "round",
    "ceil",
    "floor"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ChocolateLoverRaj/sokk"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "doc:dev": "npm run build && npm run api-extractor run --local --verbose && npm run api-documenter markdown --input-folder temp --output-folder markdown",
    "doc:release": "npm run build && npm run api-extractor run --verbose && npm run api-documenter markdown --input-folder temp --output-folder markdown",
    "lint": "npm run eslint src --ext .js,.jsx,.ts,.tsx --max-warnings=0 --fix",
    "lint:check": "npm run eslint src --ext .js,.jsx,.ts,.tsx --max-warnings=0 --fix",
    "prepare": "rm -rf dist && npm run build",
    "release": "npm run prepare && changeset publish",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,yml,yaml,css,md}": "prettier --write",
    "*.{ts}": "eslint --fix",
    "package.json": "sort-package-json"
  },
  "dependencies": {
    "@yarnpkg/pnpify": "^2.1.0"
  },
  "devDependencies": {
    "@aussiegeek/eslint-config-base": "0.1.0",
    "@changesets/cli": "^2.10.1",
    "@microsoft/api-documenter": "^7.8.33",
    "@microsoft/api-extractor": "^7.9.9",
    "@tsconfig/node12": "^1.0.7",
    "@types/eslint": "^7",
    "@types/eslint-plugin-prettier": "^3",
    "@types/jest": "^26.0.10",
    "@types/prettier": "^2",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-tsdoc": "^0.2.6",
    "fast-check": "^2.2.0",
    "husky": "^4.2.5",
    "jest": "^26.4.1",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "sort-package-json": "^1.44.0",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7"
  }
}